{
  "name": "node-server-testing",
  "version": "1.0.0",
  "description": "Node Server Testing Challenge",
  "scripts": {
    "server": "nodemon index.js",
    "test": "cross-env DB_ENV=testing jest --watch --verbose --runInBand",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "migup": "knex migrate:up",
    "migdown": "knex migrate:down",
    "seed": "knex seed:run",
    "makemigration": "knex migrate:make build-tables"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LambdaSchool/node-server-testing.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.12",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.21.21",
    "sqlite3": "^5.0.2",
    "vin-validator": "^1.0.0"
  }
}
